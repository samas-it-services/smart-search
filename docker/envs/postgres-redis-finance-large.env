# PostgreSQL + Redis - Finance Dataset - Large Size
PROVIDER_COMBO=postgres-redis-finance-large
SHOWCASE_TYPE=postgres-redis
DATASET=finance
DATA_SIZE=large

# Port assignments (5100-5199 range for this combo)
SHOWCASE_PORT=5100
POSTGRES_PORT=5101
REDIS_PORT=5102

# Database configuration
DATABASE_URL=postgresql://user:password@database:5432/smartsearch
CACHE_URL=redis://cache:6379